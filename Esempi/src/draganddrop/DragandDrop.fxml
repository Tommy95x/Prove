        </rowConstraints>
         <children>
            <Circle fx:id="circle" fill="DODGERBLUE" onDragDetected="#dragEntered" onMouseDragOver="#dragOver" onMouseDragReleased="#dragReleased" radius="56.0" stroke="BLACK" strokeType="INSIDE" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
